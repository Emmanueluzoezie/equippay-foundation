{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:dev": "lerna run build && lerna run start:dev",
    "prisma:migrate": "dotenv -e ./database/.env -- prisma migrate dev",
